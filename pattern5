
public class Main
{
	public static void main(String[] args) {
	    
	    int n=5;
	    
	    // code here
	    for (int i=0;i<n;i++){
	        
	        for(int j=1;j<=(i+1);j++){
	            if(j%2==0 && i%2==1 || j%2==1 && i%2==0)
	            System.out.print("1");
	            else
	            System.out.print("0");
	        }
	        System.out.println();
	    }
	    
}

}
