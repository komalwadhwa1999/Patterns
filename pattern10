
public class Main
{
	public static void main(String[] args) {
	    
	    int n=5;
	    
	    // code here
	    for (int i=5;i>=0;i--){
	        
	        for(int j=0;j<n-i;j++){
	            System.out.print((char)('A'+(char)(j+i))+ " ");
	        }
	        System.out.println();
	    }
	    
}

}
